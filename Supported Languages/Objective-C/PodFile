platform :ios, '6.0'

#use pod setup to update your repo if there are pods that can't be found. 
#required pods
def required_pods
    pod 'Unirest-SMASH'
    pod 'JSONModel'
	pod 'ISO8601DateFormatter', '~> 0.7'
end

target 'SMASH' do
    required_pods
end


target 'Tests' do
    required_pods
end

